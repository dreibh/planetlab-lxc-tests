#!/bin/bash
cd $(dirname $0)
mkdir -p logs
time=$(date +%H-%M)
logfile=logs/run-${time}.log
tracefile=logs/trace-${time}.txt

# figure if TestMain succeeds, as the retcod gets lost in the pipe
rm -f logs/success
python -u TestMain.py -t $tracefile "$@" &> $logfile && touch logs/success
cat $logfile

if [ -f logs/success ] ; then
    exit 0
else
    exit 1
fi
